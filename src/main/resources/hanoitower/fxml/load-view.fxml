<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hanoitower.control.LoadController">
   <children>
      <Button fx:id="myBackButton" layoutX="886.0" layoutY="546.0" mnemonicParsing="false" onAction="#attachEvents" prefHeight="40.0" prefWidth="100.0" text="Back" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="20.0" />
      <TableView fx:id="mySavedGamesTable" layoutX="100.0" layoutY="100.0" prefHeight="400.0" prefWidth="800.0">
        <columns>
          <TableColumn fx:id="myNameColumn" editable="false" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="myLevelColumn" editable="false" prefWidth="75.0" text="Level" />
            <TableColumn fx:id="myProgressColumn" editable="false" prefWidth="75.0" text="Progress" />
            <TableColumn fx:id="myModeColumn" editable="false" prefWidth="75.0" text="Mode" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
