<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ExitController">
   <children>
      <Label layoutX="55.0" layoutY="62.0" text="Do you want to save the game?" AnchorPane.leftAnchor="55.0" AnchorPane.rightAnchor="54.0" AnchorPane.topAnchor="60.0">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" layoutX="175.0" layoutY="150.0" spacing="30.0" AnchorPane.bottomAnchor="70.0" AnchorPane.topAnchor="120.0">
         <children>
            <Button fx:id="myYesButton" mnemonicParsing="false" onAction="#attachEvents" prefHeight="40.0" prefWidth="150.0" text="Yes" />
            <Button fx:id="myNoButton" mnemonicParsing="false" onAction="#attachEvents" prefHeight="40.0" prefWidth="150.0" text="No" />
         </children>
      </VBox>
   </children>
</AnchorPane>
